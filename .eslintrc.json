{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "import/prefer-default-export": "off",
        "default-case": ["error", { "commentPattern": "^skip\\sdefault" }],
        "no-console": [{ "allow": ["warn", "error"]}]
    }
}
